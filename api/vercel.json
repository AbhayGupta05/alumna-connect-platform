{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task",
    "FLASK_ENV": "production"
  },
  "functions": {
    "index.py": {
      "maxDuration": 30
    }
  }
}
